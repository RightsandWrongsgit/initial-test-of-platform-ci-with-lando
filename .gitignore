# This gitignore file was customized from the one that the standard Lando template from Platformsh provided

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
console/

# Ignore sensitive information & exposing file to host, we are doing a my-example.settings.local.php you need to look at
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore IDE files
/.idea/
/.vscode/
*.code_workspace
*.iml

# Ignore node modules
node_modules

# Ignore some CSS related stuff
*.css.map
*.css.map.map

# Ignore all log files
*.log

# Ignore .env files as they are personal
/.env

# Ignore mounts
config/sync
web/sites/default/files
tmp
private
.drush
drush-backups
.console

# Ignore default text files
/CHANGELOG.txt
/COPYRIGHT.txt
/INSTALL.txt
/LICENSE.txt
/MAINTAINERS.txt
/UPGRADE.txt
/README.txt
sites/README.txt
sites/all/modules/README.txt
sites/all/themes/README.txt

# Ignore paths that contain user-generated content
sites/*/files
sites/*/private

# Ignore directories created by Platform.sh
.subversion